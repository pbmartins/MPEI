tablesize = 1250;
array = arrayfun(@(i) hashstring(hashfunctionv2(), tablesize), 1:1000);
N = zeros(1, tablesize);
X = zeros(tablesize, 5);

for i = 0:tablesize-1
    N(i + 1) = sum(array == i);
    X(N(i + 1)+1) = X(N(i + 1)+1) + 1;
end

hist(X, [0 1 2 );
